{
    "name": "rxflow/prescription-processor",
    "description": "AI-powered prescription processing system",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.3",
        "monolog/monolog": "^2.0",
        "predis/predis": "^1.1",
        "thiagoalessio/tesseract_ocr": "^2.12",
        "google/cloud-vision": "^1.5",
        "mercadopago/dx-php": "^2.4",
        "ramsey/uuid": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src",
        "start": "php -S localhost:8080 -t public"
    }
}